    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
