{
  "name": "ocl",
  "version": "1.0.0",
  "description": "",
  "main": "./lib/index.js",
  "scripts": {
    "test": "npm run generate:parser &&  node ./node_modules/mocha/bin/mocha --compilers js:babel-core/register test/**/*.spec.js",
    "build": "npm run generate:parser && npm run build:lib && npm run build:min && npm run build:max",
    "build:lib": "rm -rf lib && babel src --out-dir lib",
    "build:min": "webpack --optimize-minimize lib/index.js dist/ocl.min.js",
    "build:max": "webpack lib/index.js dist/ocl.js",
    "build:examples": "npm run build && babel examples/examples.js -o examples/examples.min.js && webpack examples/examples.min.js examples/examples.min.js",
    "generate:parser": "rm -rf ./src/components/parser/parser.js && node generator/oclParserGenerator.js"
  },
